<?php
$a = 10;
$b = "10";
$c = 20;

// Loose comparison (==) → compares only values
var_dump($a == $b); // true (10 == "10")

// Strict comparison (===) → compares value AND type
var_dump($a === $b); // false (int vs string)

// Comparing $a and $c
var_dump($a == $c); // false (10 != 20)
var_dump($a === $c); // false

// Comparing $b and $c
var_dump($b == $c); // false ("10" != 20)
?>
